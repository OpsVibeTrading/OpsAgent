{
  "name": "be",
  "module": "index.ts",
  "private": true,
  "scripts": {
    "lint": "biome check .",
    "format": "biome format . --write",
    "api": "bun run --watch server/api.ts",
    "worker": "bun run --watch server/worker.ts",
    "build": "tsc && tsc-alias",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx lib/db/seed.ts",
    "db:seed:symbols": "npx tsx lib/db/seed-symbols.ts",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.5",
    "tsc-alias": "^1.8.16"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@biomejs/biome": "^2.2.6",
    "@google-cloud/pino-logging-gcp-config": "^1.2.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@opentelemetry/auto-instrumentations-node": "^0.66.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "ai": "^5.0.76",
    "axios": "^1.12.2",
    "bullmq": "^5.61.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "indicatorts": "^2.2.2",
    "ioredis": "^5.8.2",
    "langfuse-vercel": "^3.38.6",
    "pino-http": "^11.0.0",
    "postgres": "^3.4.7",
    "zod": "^4.1.12",
    "pino-pretty": "^13.1.2"
  }
}
